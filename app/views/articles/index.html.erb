<%= render partial: "navbar"%>
<div class="container mt-3">
  <div class="row">
    <div class="col-sm-8 offset-sm-2">
      <% @articles.each do |article| %>
      <article class="mb-3">
        <h2>
          <%= link_to article.title, article_path(article) %>
        </h2>
        <p>
          <%= article.content%>
        </p>

      </article>
      <%end%>
    </div>
  </div>
</div>
